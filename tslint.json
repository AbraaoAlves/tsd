{
	"rules": {
		"__________": [
			"tslint is still in development: some rules wait for fixes"
		],
		"bitwise": true,
		"classname": true,
		"curly": true,
		"debug": true,
		"dupkey": true,
		"duplicate-variable": false,
		"eofline": true,
		"eqeqeq": [true, "allow-null-check"],
		"evil": true,
		"forin": false,
		"indent": [true, 4],
		"labelpos": true,
		"label-undefined": true,
		"maxlen": [true, 240],
		"noarg": true,
		"noconsole": [true,
			"debug",
			"info",
			"time",
			"timeEnd",
			"trace"
		],
		"noconstruct": true,
		"nounreachable": true,
		"noempty": false,
		"oneline": [true,
			"check-open-brace",
			"check-whitespace"
		],
		"quotemark": [true, "single"],
		"radix": true,
		"semicolon": true,
		"sub": false,
		"trailing": true,
		"varname": false,
		"____________________": {
			"whitespace": [
				"check-separator",
				"check-type"
			]
		},
		"whitespace": [true,
			"check-branch",
			"check-decl",
			"check-operator"
		]
	}
}
