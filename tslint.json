{
	"rules": {
		"bitwise": true,
		"classname": true,
		"curly": true,
		"debug": true,
		"dupkey": true,
		"eofline": false,
		"eqeqeq": true,
		"evil": true,
		"forin": true,
		"___indent": [false, 4],
		"labelpos": true,
		"labelundef": true,
		"__maxlen should be lower__": "but legacy code",
		"maxlen": [true, 280],
		"noarg": true,
		"noconsole": [true, "debug, info, time, timeEnd, trace"],
		"noconstruct": true,
		"noempty": false,
		"oneline": [false, "check-whitespace"],
		"quotemark": [true, "single"],
		"radix": true,
		"semicolon": true,
		"sub": false,
		"trailing": true,
		"varname": false,
		"whitespace": false
	}
}